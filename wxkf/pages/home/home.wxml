<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" style='height:{{height*2}}rpx;width:100%;' mode="aspectFill"/>
    </swiper-item>
  </block>
</swiper>
